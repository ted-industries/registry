{
  "version": 2,
  "name": "ted-registry-proxy",
  "rewrites": [
    {
      "source": "/",
      "destination": "https://extensions.ted.tomlin7.com/v1/extensions.json"
    },
    {
      "source": "/v1",
      "destination": "https://extensions.ted.tomlin7.com/v1/extensions.json"
    },
    {
      "source": "/v1/extensions.json",
      "destination": "https://extensions.ted.tomlin7.com/v1/extensions.json"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}